{
  "name": "node_intensive",
  "module": "index.ts",
  "type": "module",
  "scripts": {
    "dev": "bun run index.ts",
    "build": "bun build --outdir=dist --target=node16 --format=cjs --minify",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
